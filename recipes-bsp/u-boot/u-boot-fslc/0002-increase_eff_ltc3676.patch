From 8f5edfbac20e819f5227e78bb4237bbe01bfde00 Mon Sep 17 00:00:00 2001
From: Stefano Gurrieri <stefano.gurrieri@roj.com>
Date: Fri, 18 Nov 2016 09:13:45 +0100
Subject: [PATCH] increase_eff_ltc3676

---
 board/freescale/mx6qroj/mx6qroj.c | 12 ++++++++++--
 1 file changed, 10 insertions(+), 2 deletions(-)

diff --git a/board/freescale/mx6qroj/mx6qroj.c b/board/freescale/mx6qroj/mx6qroj.c
index a60109d..6ab1855 100644
--- a/board/freescale/mx6qroj/mx6qroj.c
+++ b/board/freescale/mx6qroj/mx6qroj.c
@@ -633,16 +633,24 @@ int board_init(void)
 			reg = 0x13 | LTC3676_PGOOD_MASK;
 			pmic_reg_write(p, LTC3676_DVB1B, reg);
 			/* set SW1 (VDD_SOC) to 1275mV */
-			reg = 0x13;
+			reg = 0x16;
 			pmic_reg_write(p, LTC3676_DVB1A, reg);
 
 			/* mask PGOOD during SW3 transition */
 			reg = 0x13 | LTC3676_PGOOD_MASK;
 			pmic_reg_write(p, LTC3676_DVB3B, reg);
 			/*set SW3 (VDD_ARM) to 1275mV */
-			reg = 0x13;
+			reg = 0x16;
 			pmic_reg_write(p, LTC3676_DVB3A, reg);
 		}
+		reg = 0x20;
+		pmic_reg_write(p, LTC3676_BUCK1, reg);
+		reg = 0x20;
+		pmic_reg_write(p, LTC3676_BUCK2, reg);
+		reg = 0x20;
+		pmic_reg_write(p, LTC3676_BUCK3, reg);
+		reg = 0x20;
+		pmic_reg_write(p, LTC3676_BUCK4, reg);		
 	}
 #endif
 #ifdef CONFIG_CMD_SATA
