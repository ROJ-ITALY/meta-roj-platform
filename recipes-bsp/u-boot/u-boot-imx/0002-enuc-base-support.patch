 	imx6dl-sabreauto.dtb \
@@ -338,6 +339,7 @@ dtb-$(CONFIG_MX6) += imx6ul-14x14-ddr3-arm2.dtb \
 	imx6dl-sabreauto-gpmi-weim.dtb \
 	imx6dl-sabresd.dtb \
 	imx6q-arm2.dtb \
+	imx6q-enuc.dtb \
 	imx6q-pop-arm2.dtb \
 	imx6q-icore.dtb \
 	imx6q-icore-rqs.dtb \
